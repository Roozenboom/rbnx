{
  "$schema": "http://json-schema.org/schema",
  "name": "webdriverio",
  "version": "0.0.1",
  "generators": {
    "init": {
      "factory": "./src/generators/init/generator",
      "schema": "./src/generators/init/schema.json",
      "description": "Add wdio configuration and dependencies to the workspace",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "project": {
      "factory": "./src/generators/project/generator",
      "schema": "./src/generators/project/schema.json",
      "description": "project generator"
    }
  },
  "schematics": {
    "init": {
      "factory": "./src/generators/init/compat",
      "schema": "./src/generators/init/schema.json",
      "description": "Add wdio configuration and dependencies to the workspace",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "project": {
      "factory": "./src/generators/project/compat",
      "schema": "./src/generators/project/schema.json",
      "description": "project generator"
    }
  }
}
